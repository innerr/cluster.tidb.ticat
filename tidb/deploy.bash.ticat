help = deploy tiup cluster name and then set the name to env
abbrs = depl|dep|dpl

[env]
tidb.tiup.yaml = may-read|write
tidb.cluster = may-read|write
tidb.version = write

[args]
topology|yaml|yam|path|file|f|F|p|P|y|Y = ''
cluster-name|cluster|name|n|N = ''
version = nightly
confirm|cfm|c|C = true

[dep]
tiup = to deploy cluster
